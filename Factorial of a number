#include<stdio.h>
int main()
{
int n,i;
unsigned long long factorial=1;
printf("Enter a non-negative integer:");
scanf("%d",&n);
if(n<0)
{
printf("Factorial is not defined for negative numbers.\\n");
}
else
{
for(i=1;i<=n;i++)
{
factorial*=i;
}
printf("Factorial of %d=%11u\n",n,factorial);
}
return 0;
}

OUTPUT:
Enter a non-negative integer:5
Factorial of 5= 120
